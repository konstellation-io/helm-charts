{
    "language": "python",
    "display_name": "KAI Python 3.9 Tensorflow",
    "metadata": {
      "process_proxy": {
        "class_name": "enterprise_gateway.services.processproxies.k8s.KubernetesProcessProxy",
        "config": {
          "image_name": "localhost:32000/konstellation/jupyter-flavors-kai-python-tf:latest"
        }
      }
    },
    "env": {
      "PYTHONPATH": "$HOME/.local/lib/python3.6/site-packages"
    },
    "argv": [
      "python",
      "/usr/local/share/jupyter/kernels/jupyter-flavors-kai-python-tf/scripts/launch_kubernetes.py",
      "--RemoteProcessProxy.kernel-id",
      "{kernel_id}",
      "--RemoteProcessProxy.port-range",
      "{port_range}",
      "--RemoteProcessProxy.response-address",
      "{response_address}"
    ]
  }
