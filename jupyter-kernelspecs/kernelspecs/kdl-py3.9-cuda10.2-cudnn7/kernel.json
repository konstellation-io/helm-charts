{
  "language": "python",
  "display_name": "KDL Python 3.9 - CUDA 10.2 - cuDNN 7",
  "metadata": {
    "process_proxy": {
      "class_name": "enterprise_gateway.services.processproxies.k8s.KubernetesProcessProxy",
      "config": {
        "image_name": "konstellation/kdl-py:3.9-cuda10.2-cudnn7"
      }
    }
  },
  "env": {
    "KERNEL_PIP_TARGET": "/kernel/lib/python/site-packages",
    "KERNEL_PYTHONPATH": "/kernel/lib/python/site-packages"
  },
  "argv": [
    "python",
    "/usr/local/share/jupyter/kernels/kdl-py3.9-cuda10.2-cudnn7/scripts/launch_kubernetes.py",
    "--RemoteProcessProxy.kernel-id",
    "{kernel_id}",
    "--RemoteProcessProxy.port-range",
    "{port_range}",
    "--RemoteProcessProxy.response-address",
    "{response_address}"
  ]
}
